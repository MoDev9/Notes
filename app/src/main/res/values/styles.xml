<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textColorPrimary">#20123A</item>
        <item name="android:textColorSecondary">#737373</item>
        <item name="android:windowBackground">#F9F9FB</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:statusBarColor">#F9F9FB</item>
        <item name="android:colorEdgeEffect">#312A65</item>
        <item name="android:colorAccent">#F9F9FB</item>
        
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <style name="TexApp" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">#212121</item>
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
    </style>

</resources>
